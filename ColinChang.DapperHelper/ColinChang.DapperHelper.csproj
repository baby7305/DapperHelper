<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Colin Chang</Authors>
    <Description>A common dapper helper based on Dapper.</Description>
    <Copyright>https://colin-chang.site/</Copyright>
    <PackageLicenseUrl>https://raw.githubusercontent.com/colin-chang/DapperHelper/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/colin-chang/DapperHelper</PackageProjectUrl>
    <RepositoryUrl>https://github.com/colin-chang/DapperHelper</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>dapper,dapperplus,dapperhelper,dapper.net</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Dapper" Version="1.50.5" />
  </ItemGroup>
</Project>
