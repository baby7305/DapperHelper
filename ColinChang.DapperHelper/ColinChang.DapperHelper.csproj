<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Colin Chang</Authors>
    <Description>This is a common utility based on Dapper,it just provides some useful help for common CRUD operations.</Description>
    <Copyright>https://colin-chang.site/</Copyright>
    <PackageLicenseUrl></PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/colin-chang/DapperHelper</PackageProjectUrl>
    <RepositoryUrl>https://github.com/colin-chang/DapperHelper</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>dapper,dapperplus,dapperhelper,dapper.net</PackageTags>
    <Version>1.0.1</Version>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageReleaseNotes>1.update dependencies
2.rename main object to "DapperHelper"</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Dapper" Version="1.60.6" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
</Project>
